# GitHub Personal Access Token
# Get from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# Gemini API Key
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_db_password_here
DB_NAME=backstage

# Container Registry
# Example for Docker Hub: registry.hub.docker.com/your-username
# Example for local registry: localhost:5000
CONTAINER_REGISTRY_URL=localhost:5000
CONTAINER_REGISTRY_USER=your_registry_username
CONTAINER_REGISTRY_PASSWORD=your_registry_password

# Kubernetes Cluster Configuration
# API Server URL - aman di .env karena tidak di-commit ke git
K8S_API_SERVER=https://103.185.52.178:6443
K8S_SERVICE_ACCOUNT_TOKEN=your_k8s_service_account_token
K8S_NAMESPACE=justpush-apps

# Kong API Gateway Configuration
# Kong Admin API URL (internal, for configuration)
KONG_ADMIN_URL=http://kong-admin:8001
# Kong Admin Token (optional, if Kong has authentication enabled)
KONG_ADMIN_TOKEN=your_kong_admin_token
# Kong Gateway URL (public, for API access)
KONG_GATEWAY_URL=http://kong-gateway:8000
